1) To read other peoples messages, run:
var socket = io();
socket.on('tooMany', function(data){
		console.log(data)
	});

for (i = 0; i < 100000; i++) { socket.emit('load', i);}


var socket = io();
socket.on('tooMany', function(data){
		console.log(data)
	});

for (i = 0; i < 10000; i++) { socket.emit('load', i);}